<?xml version="1.0"?>
<block>
	<name>Wireshark Connector</name>
	<key>ccs_wireshark_connector</key>
	<category>CCS</category>
	<import>import ccs</import>
	<make>ccs.wireshark_connector($debug)</make>

	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>

	<sink>
		<name>in</name>
		<type>message</type>
	</sink>
	<source>
		<name>out</name>
		<type>byte</type>
		<nports>1</nports>
	</source>
</block>
